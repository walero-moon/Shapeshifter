{
  "name": "shapeshifter-bot",
  "version": "0.1.0",
  "description": "A PluralKit-like Discord bot for message proxying.",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "cross-env ESLINT_USE_FLAT_CONFIG=false eslint .",
    "typecheck": "tsc --noEmit",
    "register:commands": "tsx scripts/register-commands.ts",
    "drizzle:generate": "drizzle-kit generate",
    "drizzle:migrate": "tsx src/db/migrate.ts",
    "test": "vitest",
    "test:run": "vitest run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/walero-moon/Shapeshifter.git"
  },
  "keywords": [
    "discord",
    "bot",
    "plurality",
    "pluralkit"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/walero-moon/Shapeshifter/issues"
  },
  "homepage": "https://github.com/walero-moon/Shapeshifter#readme",
  "dependencies": {
    "better-sqlite3": "^12.4.1",
    "discord.js": "^14.24.2",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.44.7",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^24.9.2",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "cross-env": "^10.1.0",
    "drizzle-kit": "^0.31.6",
    "eslint": "^9.39.0",
    "eslint-plugin-import": "^2.32.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^4.0.6"
  }
}
